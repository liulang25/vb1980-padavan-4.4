msgid " <dev>                     device (usually disk) path\n"
msgstr " <disp>                    caminho do dispositivo (habitualmente o disco)\n"

#: disk-utils/sfdisk.c:2152
msgid " <part>                    partition number\n"
msgstr " <part>                    número da partição\n"

